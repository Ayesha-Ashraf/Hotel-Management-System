@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">
        Hotel Management </a>.</p>
</div>
@foreach (var item in Model.rooms)
{
    <h3>@item.name</h3>
    <h3>@item.room_id</h3>
    <img src="@item.image" />

}